body {
  padding-top: 5rem;
}
.starter-template {
  padding: 3rem 1.5rem;
  text-align: center;
}

nav {
  height: 4em;
}

nav img {
  padding-top: 40px;
  border: rgb(33, 37, 41) 6px solid;
}

article h5 {
  margin-top: -35px;
  background-color: white;
  width: 200px;
}

.bd-callout {
    padding: 1.25rem;
    margin-top: 1.25rem;
    margin-bottom: 1.25rem;
    border: 1px solid #eee;
        border-left-width: 1px;
    border-left-width: .25rem;
    border-radius: .25rem;
}

.bd-callout-warning {
    border-left-color: #f0ad4e;
}
